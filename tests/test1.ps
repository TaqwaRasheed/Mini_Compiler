a = 0:
b = 1:
max = 50:
    
plot "--- Fibonacci Sequence ---":
loop i in 1..10 {
    check a > max {
        plot "Reached Limit!":
        note> This trick stops the loop by pushing iterator to end
        i = 100: 

    } else {
        plot a:
        temp = a + b:
        a = b:
        b = temp:
    }
}